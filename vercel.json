{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "/routes/api.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "/routes/users.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/routes/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/routes/index.js"
    }
  ]
}
